    "@mui/material": "^7.0.2",
    "@mui/x-date-pickers": "^7.28.3",
    "@tanstack/react-query": "^5.74.3",
    "axios": "^1.8.4",
    "dayjs": "^1.11.13",
    "extenso": "^2.1.0",
    "pdfmake": "^0.2.18",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.55.0",
    "react-number-format": "^5.4.4",
    "yup": "^1.6.1"
