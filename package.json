    "@typescript-eslint/parser": "^8.30.0",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "@vitest/ui": "^3.1.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "git-commit-msg-linter": "^5.0.8",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
